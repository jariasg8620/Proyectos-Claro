	<!-- This is the DB Data block -->
	<form id="DBData7">
		<var name="AppStateString" expr="'(' + JSON.stringify(AppState) + ')'" />
		<var name="db_query_type" expr="'SQLScriptFile'" />
		<!-- branch off based on the query type: SQL Query, Query File or Stored Procedure -->	
		<var name="db_query" expr="'NavegacionNM.sql'" />		
		<var name="db_name" expr="'ConnectionDB'" />
		<var name="db_timeout" expr="20" />
		
		
    	<block>
			<data name="DBData7Data" src="../include/dbrequest.jsp" method="post" 
				namelist="AppStateString db_query db_query_type db_name db_timeout"
				 enctype="multipart/form-data" 
				 fetchtimeout="22s"/>
				
        	<if cond="typeof DBData7Data.errorMsg == 'undefined'">
            	<if cond="typeof DBData7Data.db_result != 'undefined'">
            		
            		<if cond="DBData7Data.db_result.length == 0">
            			<throw event="error.com.genesyslab.composer.dbemptyresultset" messageexpr="'No records returned from database'" />
            		</if>
            		
            		<assign name="AppState.DBData7DBResult" expr="DBData7Data.db_result" />
            		
            		<assign name="AppState.DBData7DBResultColumnNames" expr="DBData7Data.db_result_columns" />

            	</if>
            	<if cond="typeof DBData7Data.db_outparams != 'undefined'">
         			<log expr="'db_outparams:' + DBData7Data.db_outparams"/>
					
            	</if>
    			<goto next="#PMP_DESPEDIDA" />
        	<else/>
        	    <if cond="typeof DBData7Data.errorType != 'undefined'">
        	        <if cond="DBData7Data.errorType == 'timeout'">
    	        	    <throw event="error.com.genesyslab.composer.dbtimeout" messageexpr="DBData7Data.errorMsg" />
    	            <elseif cond="DBData7Data.errorType == 'connectionerror'" />
    	        	    <throw event="error.com.genesyslab.composer.dbconnectionerror" messageexpr="DBData7Data.errorMsg" />
    	            </if>
        		<else/>
	        		<throw event="error.com.genesyslab.composer.dberror" messageexpr="DBData7Data.errorMsg" />
        		</if>
        	</if>
		</block>
    	<catch event="error.com.genesyslab.composer.dberror">
    		<goto next="#PMP_DESPEDIDA" />
        </catch>
    	<catch event="error.com.genesyslab.composer.dbemptyresultset">
    		<goto next="#PMP_DESPEDIDA" />
        </catch>
    	<catch event="error.com.genesyslab.composer.dbtimeout">
    		<goto next="#PMP_DESPEDIDA" />
        </catch>
    	<catch event="error.com.genesyslab.composer.dbconnectionerror">
    		<goto next="#PMP_DESPEDIDA" />
        </catch>
    	<catch event="error.com.genesyslab.composer.variablemappingerror">
    		<goto next="#PMP_DESPEDIDA" />
        </catch>
	</form>

