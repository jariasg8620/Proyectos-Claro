					var m_TipoId = AppState.m_TipoId;
					var m_Nombre = AppState.m_Nombre;
					var m_Fecha = AppState.m_Fecha;
				</script>
		    	<gvp:send namelist="m_Movimiento m_Ani m_ColaVQ m_Identificacion m_IdGvp m_IdOrs m_Opcion m_Origen m_TipoId m_Nombre m_Fecha" async="false"/>
			<else/>
				<!--  CTIC Case -->
				<script>
					var Action = "AttachData";
					var sub_action = "Add";
					var m_Movimiento = AppState.m_Movimiento;
					var m_Ani = AppState.m_Ani;
					var m_ColaVQ = AppState.m_ColaVQ;
					var m_Identificacion = AppState.m_Identificacion;
					var m_IdGvp = AppState.m_IdGvp;
					var m_IdOrs = AppState.m_IdOrs;
					var m_Opcion = AppState.m_Opcion;
					var m_Origen = AppState.m_Origen;
					var m_TipoId = AppState.m_TipoId;
					var m_Nombre = AppState.m_Nombre;
					var m_Fecha = AppState.m_Fecha;
				</script>
				<gvp:send namelist="Action sub_action m_Movimiento m_Ani m_ColaVQ m_Identificacion m_IdGvp m_IdOrs m_Opcion m_Origen m_TipoId m_Nombre m_Fecha" async="false"/>
    			<gvp:receive maxtime="10s"/>
				<if cond="isCTICResult( application.lastmessage$ ) == 'false'">
    				<throw event="error.com.genesyslab.composer.receiveerror" messageexpr="'The received message has invalid content-type.'" />
    			</if>
    			
    			<var name="EnvioDataMessage" expr="application.lastmessage$.content"/>
				<var name="arrUserData" expr="getUserDataArray(EnvioDataMessage)"/>
				<var name="EnvioDataResult" expr="utilGetReceiveDataValue('Result', arrUserData)" />
                
				<if cond="EnvioDataResult != 'SUCCESS'">
					<var name="EnvioDataResultReason" expr="utilGetReceiveDataValue('reason', arrUserData)" />
					<if cond="EnvioDataResultReason == 'Timeout'">
						<throw event="error.com.genesyslab.composer.operationtimeout" messageexpr="EnvioDataResultReason" />
					<else/>
						<throw event="error.com.genesyslab.composer.receiveerror" messageexpr="EnvioDataResultReason" />
					</if>
		 		</if>
			</if>
    		<goto next="#Transfer" />
  		</block>
	</form>

